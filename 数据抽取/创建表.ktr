        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import org.pentaho.di.core.database.DatabaseMeta;
import java.util.*;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException 
{ 
    // First, get a row from the default input hop 
    //
       String tt = "";
      Object[] r = getRow();
  if (r == null) {
    setOutputDone();
    return false;
  } 
 logBasic("***************************"+Boolean.FALSE.toString());
       
           
            String tablename = getVariable("TABLE_NAME");
			String tableID = getVariable("TABLE_ID");

            String database = getVariable("DATA_BASE");
            String organization = getVariable("organization","dbo");
            String lastdate = getVariable("up_date", "");
            String dataBaseType = getVariable("SOURCE_DB_TYPE");
			
    		String imSer = getVariable("SOURCE_DB_SERVER");
			String impo = getVariable("SOURCE_DB_PORT");
			String imDB = getVariable("SOURCE_DB_NAME");
			String imUser = getVariable("SOURCE_DB_USER");
			String imPwd = getVariable("SOURCE_DB_PWD");
			String useDB = getVariable("USEDB_NAME");


    		String loSer = getVariable("TARGET_DB_SERVER");
			String loDB = getVariable("TARGET_DB_NAME");
			String lopo = getVariable("TARGET_DB_PORT");
			String loUser = getVariable("TARGET_DB_USER");
            String loPwd = getVariable("TARGET_DB_PWD");
			
			String crsql = "";

			DatabaseMeta dbmeta_im = new DatabaseMeta();

			DatabaseMeta dbmeta = new DatabaseMeta();

			logBasic("******++++++");


 	dbmeta_im = getTransMeta().findDatabase("SOURCE_LINK");
                  
		dbmeta = getTransMeta().findDatabase("TARGET_LINK");
		logBasic("设置本地连接");

//dbmeta_im.setConnectSQL("jdbc:sqlserver://"+loSer+";databaseName="+loDB+";integratedSecurity=false123");
 
//dbmeta.setConnectSQL("jdbc:sqlserver://"+loSer+";databaseName="+loDB+";integratedSecurity=false123");

		logBasic(imSer+"11111111");
		dbmeta_im.setHostname( imSer );
		logBasic(imDB+"222222222");
    	dbmeta_im.setDBName( imDB );
    	dbmeta_im.setDBPort( impo );
    	dbmeta_im.setUsername( imUser );
    	dbmeta_im.setPassword( imPwd );

		dbmeta.setHostname( loSer );
    	dbmeta.setDBName( loDB );
    	dbmeta.setDBPort( lopo );
    	dbmeta.setUsername( loUser );
    	dbmeta.setPassword( loPwd );

logBasic(dbmeta.getHostname()+"***************************");
org.pentaho.di.core.database.Database db = null;
org.pentaho.di.core.database.Database db_im = null;	
try{
            db=new org.pentaho.di.core.database.Database(dbmeta);
			db_im=new org.pentaho.di.core.database.Database(dbmeta_im);

}
catch(Exception e){
logBasic(e.toString());
}
        if(dataBaseType.equals("MSSQLNATIVE"))
             tt = imDB +"."+organization+"."+tablename;
                     else
                      tt = useDB+"."+tablename;     
 
           logBasic(dbmeta_im.getConnectSQL()+"--------------------");
			logBasic(dbmeta.getConnectSQL()+"--------------------");
                    
                     
                    
		logBasic(tt+"--------------------");
        try 
        { 

			db_im.connect();
            db.connect(); 
           
            
            logBasic("开始创建表：" + tablename); 
			StringBuilder retval = new StringBuilder();
			retval.append("INSERT INTO ETL.DBO.CRSQL (TABLE_ID,TABLE_NAME,COLUMN_NAME,COLUMN_TYPE) VALUES ");
             
            if(tablename!=null &amp;&amp; tablename.trim().length()>0) 
            { 
                //String sql = db.getDDLCreationTable(tablename, data.inputRowMeta);//${TABLENAME} 
                crsql = db.getDDLCreationTable(tablename, db_im.getTableFields(tt));
				//crsql = crsql.replaceAll("VARCHAR\\([0-9]+\\)","VARCHAR(max)").replace(";", "");
                crsql = crsql.replaceAll("CREATE TABLE","INSERT INTO ELT.DBO.CRSQL");   
				RowMetaInterface fields = db_im.getTableFields(tt); 
                //db.execStatement(sql); 
 
                //logBasic(db_im.getPrimaryKeyColumnNames(tablename)[0]+"sssssqlllsqlll"); 
				//logBasic(db_im.databaseMeta+"sssssqlllsqlll"); 
			for(int i = 0; i &lt; fields.size(); ++i) {
           

            ValueMetaInterface v = fields.getValueMeta(i);
			String sss = "('"+tablename+"','"+tableID+"','"+dbmeta_im.getFieldDefinition(v, null, null, true)+"')";
			sss = sss.replace(" ","','").replaceAll("\\s","");
			if (i > 0) {
                retval.append(", "+sss);
            } else {
                retval.append(sss);
            }
            

       			}

			crsql = retval.toString();

			logBasic(crsql);

            } 
        } 
        catch(Exception e) 
        {       throw new Exception("2222ewew"+e+"11111") ;    
            //logError("创建表出现异常11111"+db.getTableFields(tablename),e); 
             
        }finally{ 
            db.disconnect(); 
                     db_im.disconnect();
        } 
  get(Fields.Out, "crsql").setValue(r, crsql);
  putRow(data.outputRowMeta, r);
    
    return true; 
} 
